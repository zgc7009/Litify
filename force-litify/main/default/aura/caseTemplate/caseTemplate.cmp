<aura:component
    implements="lightning:appHomeTemplate"
    description="Case List Template"
    access="global">
    <aura:attribute name="main" type="Aura.Component[]" access="global" />

    <div aura:id="container">
        <lightning:layout>
            <lightning:layoutItem
                aura:id="main"
                size="12">
                {!v.main}
            </lightning:layoutItem>
        </lightning:layout>
    </div>
</aura:component>